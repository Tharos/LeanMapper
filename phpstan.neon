parameters:
    level: 7

    paths:
        - src
        - tests

    fileExtensions:
        - php

    excludePaths:
        - tests/tmp

    treatPhpDocTypesAsCertain: false

    ignoreErrors:
        - '#Unsafe usage of new static\(\).#'


services:
    -
        class: LeanMapper\Tests\PhpStan\DibiFluentDynamicReturnTypeExtension
        tags:
            - phpstan.broker.dynamicMethodReturnTypeExtension
